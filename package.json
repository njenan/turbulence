{
  "name": "turbulence",
  "version": "1.0.0",
  "description": "A plug-in based, distributed performance testing tool for developers",
  "main": "index.js",
  "scripts": {
    "postinstall": "tsd install",
    "package": "tsc src/*.ts src/**/*.ts --module commonjs",
    "test": "tsc src/*.ts src/**/*.ts; mocha src/**/__test__/*.js",
    "watch": "watch tsc src/*.ts src/**/*.ts; mocha src/**/__test__/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/njenan/turbulence.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/njenan/turbulence/issues"
  },
  "homepage": "https://github.com/njenan/turbulence#readme",
  "devDependencies": {
    "mocha": "^2.4.5",
    "power-assert": "^1.3.1",
    "q": "^1.4.1",
    "typescript": "^1.8.7",
    "watch": "^0.17.1"
  }
}
