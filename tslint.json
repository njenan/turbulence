{
  "extends": "tslint:recommended",
  "rules": {
    "typedef-whitespace": "nospace",
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": false,
    "forin": false,
    "trailing-comma": [
      false
    ],
    "switch-default": false,
    "no-reference": true,
    "only-arrow-functions": [
      true,
      "allow-declarations"
    ]
  }
}